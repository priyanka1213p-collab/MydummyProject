package Basics2;

public class if_else_elseif {
	public static void main(String[] args) {
		int n =10;
		if (n>0)
		{
			System.out.println("number is positive ");
		}
		
		int k =8;
		if(k % 2 == 0)
		{
			System.out.println("Even number ");
		}
		else 
		{
			System.out.println("odd number");
		}
		
	
	}

}
